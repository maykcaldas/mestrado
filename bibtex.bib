1530-6984 (Linking)},
issn = {15306992},
journal = {Nano Lett.},
keywords = {C-C breaking,MCP,Nanoparticle,dendrimer,heterogeneous catalysis},
number = {3},
pages = {1853--1862},
pmid = {28151681},
title = {{Platinum and Other Transition Metal Nanoclusters (Pd, Rh) Stabilized by PAMAM Dendrimer as Excellent Heterogeneous Catalysts: Application to the Methylcyclopentane (MCP) Hydrogenative Isomerization}},
volume = {17},
