doi = {10.1063/1.4941379},
file = {:home/mayk/Mendeley Desktop/Smeijers et al/2016/Smeijers et al. - 2016 - Coarse-grained simulations of poly(propylene imine) dendrimers in solution.pdf:pdf},
isbn = {doi:10.1063/1.4941379},
