title = {{Synthesis and self-assembly of PAMAM/PAA Janus dendrimers}},
volume = {1},
year = {2014}
}
@article{Gilat1999,
abstract = {A light-harvesting antenna was made by functionalizing a dendrimer with suitable chromophores. In the dye-labeled dendrimers, light absorbed by the peripheral chromophores is funneled to a central fluorescent core with high efficiency and by a mechanism that is independent of the dendritic architecture. This scheme is reminiscent of photosynthesis, where solar energy absorbed anywhere in an array of hundreds of chlorophyll mols. is efficiently transferred to a single reaction center. The dye-labeled poly(aryl ether) dendrons G1 to G4 were prepd. by nucleophilic attack of coumarin 2 on the benzyl bromide groups of protected monomer; the deprotected phenol was coupled to coumarin 343 to produce the fully dye-labeled first generation dendron. The deprotection and coupling steps were repeated to obtain the G-2 and G-3 dendrons functionalized with both coumarin 2 and coumarin 343. The coupling of the fourth generation phenol to coumarin 343 produced the fully dye-labeled G-4 macromol. The optical absorption of the macromol. at each generation closely matched the sum of contributions of individual fragments; the absorption capacity of the dendrimer increases with the no. of terminal chromophores, and the light-harvesting antenna becomes more and more effective as the dendrimer generation increases. The dendrimers also display efficient energy transfer up to the fourth generation. Almost all of the absorbed energy is used to excite the focal dye and is eventually converted into a single emission spectrum. At high generations (G-3 and G-4), the spatial concn. of energy results in a net fluorescence amplification. This energy transfer provides unprecedented flexibility since any dendritic architecture can, in principle, be used to build this type of photoactive device. [on SciFinder(R)]},
author = {Gilat, Sylvain L. and Adronov, Alex and Fr{\'{e}}chet, Jean M.J.},
doi = {10.1002/(SICI)1521-3773(19990517)38:10<1422::AID-ANIE1422>3.0.CO;2-V},
file = {:home/mayk/Mendeley Desktop/Gilat, Adronov, Fr{\'{e}}chet/1999/Gilat, Adronov, Fr{\'{e}}chet - 1999 - Light harvesting and energy transfer in novel convergently constructed dendrimers.pdf:pdf},
isbn = {1433-7851},
issn = {14337851},
journal = {Angew. Chemie - Int. Ed.},
keywords = {Chromophores,Dendrimers,Energy transfer,Light harvesting,Molecular devices},
number = {10},
pages = {1422--1427},
title = {{Light harvesting and energy transfer in novel convergently constructed dendrimers}},
volume = {38},
year = {1999}
}
