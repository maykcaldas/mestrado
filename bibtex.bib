}
@article{Bosko2011,
abstract = {The effect of molecular topology on the structural and dynamic properties$\backslash$nof dendrimers with different generation numbers and molecular weights$\backslash$nis studied in solvents of varying solvent quality and compared with$\backslash$nthe behavior of linear polymer chains of similar molecular weights.$\backslash$nBrownian dynamics simulations are used, with a coarse-grained bead-spring$\backslash$nchain model for the dendrimer, and both intramolecular hydrodynamic$\backslash$nand excluded volume interactions are incorporated. The successive$\backslash$nfine graining technique is applied to obtain parameter free predictions$\backslash$nof properties as a function of solvent quality. In particular, it$\backslash$nis shown that with a suitable definition of the solvent quality parameter,$\backslash$nthe swelling of dendrimers with increasing solvent quality can be$\backslash$ncollapsed on to a single master curve for molecules with the same$\backslash$ntopology. The swelling of dendrimers is found to cause a decrease$\backslash$nin the translational diffusivity, and a shift in the characteristic$\backslash$nmaximum in intrinsic viscosity with molecular mass to higher generation$\backslash$nnumbers. Ratios of the radius of gyration to the hydrodynamic and$\backslash$nviscometric radii converge to constant universal values for dendrimers$\backslash$nwith a fixed degree of branching.},
author = {Bosko, Jaroslaw T. and Prakash, J. Ravi},
doi = {10.1021/ma102094f},
file = {:home/mayk/Mendeley Desktop/Bosko, Prakash/2011/Bosko, Prakash - 2011 - Universal behavior of dendrimer solutions.pdf:pdf},
isbn = {0024-9297},
