doi = {10.1021/ma970974k},
file = {:home/mayk/Mendeley Desktop/Ramzi et al/1998/Ramzi et al. - 1998 - Intermolecular interactions between dendrimer molecules in solution studied by small-angle neutron scattering.pdf:pdf},
