journal = {Anal. Biochem.},
keywords = {Cytotoxicity,Dendrimer,In vitro release profile,Nanoparticle,Neuro 2A,Sol-gel},
month = {feb},
pages = {1--7},
pmid = {27939389},
publisher = {Elsevier Inc},
title = {{Synthesis of silica-PAMAM dendrimer nanoparticles as promising carriers in Neuro blastoma cells}},
url = {http://dx.doi.org/10.1016/j.ab.2016.12.004 https://linkinghub.elsevier.com/retrieve/pii/S0003269716304080},
