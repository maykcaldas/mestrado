author = {Nisato, Giovanni and Ivkov, Robert and Amis, Eric J.},
doi = {10.1021/ma991474p},
file = {:home/mayk/Mendeley Desktop/Nisato, Ivkov, Amis/2000/Nisato, Ivkov, Amis - 2000 - Size invariance of polyelectrolyte dendrimers.pdf:pdf},
