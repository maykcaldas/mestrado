journal = {Fuel},
keywords = {Adsorption,Co(II),DFT,PAMAM dendrimers,Silica-gel},
pages = {91--101},
publisher = {Elsevier Ltd},
title = {{Removal of Co(II) from fuel ethanol by silica-gel supported PAMAM dendrimers: Combined experimental and theoretical study}},
url = {http://dx.doi.org/10.1016/j.fuel.2017.02.076},
volume = {199},
