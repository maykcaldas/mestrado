@article{Caballero2013,
abstract = {The interaction of poly(amidoamine)-G3 (PAMAM-G3) dendrimer with nicotinic acid (NA) was investigated by using molecular dynamics (MD) simulations. First, sample free energy profiles of NA crossing PAMAM-G3 at pH 6 and 3 were computed using the adaptive biasing force (ABF) method. We found that PAMAM-G3 provides a more appropriate environment for NA inclusion when internal tertiary amine groups are unprotonated (at pH 6). However, when internal tertiary amine groups are protonated (at pH 3), the PAMAM cavities are less hydrophobic; therefore the drug-dendrimer interactions become similar to drug-solvent interactions. Traditional MD simulations were also performed to investigate the structural stability of the PAMAM-NA complexes near the free energy minima at pH 6. We found that association of NA and PAMAM adopts a preferred binding mode around the surface of PAMAM, where hydrogen bond (HB) interactions with the amino and amide NH groups of the nearby monomers are established. These interactions are very stable whether additional van der Waals interactions between pyridine ring of NA and methylene groups of the more external monomers of PAMAM are established. {\textcopyright} 2012 Elsevier Inc.},
author = {Caballero, Julio and Poblete, Horacio and Navarro, Cristell and Alzate-Morales, Jans H.},
doi = {10.1016/j.jmgm.2012.11.003},
file = {:home/mayk/Mendeley Desktop/Caballero et al/2013/Caballero et al. - 2013 - Association of nicotinic acid with a poly(amidoamine) dendrimer studied by molecular dynamics simulations.pdf:pdf},
issn = {10933263},
journal = {J. Mol. Graph. Model.},
keywords = {Dendrimer,Molecular dynamics,Nicotinic acid,PAMAM},
pages = {71--78},
pmid = {23220284},
publisher = {Elsevier Inc.},
title = {{Association of nicotinic acid with a poly(amidoamine) dendrimer studied by molecular dynamics simulations}},
url = {http://dx.doi.org/10.1016/j.jmgm.2012.11.003},
volume = {39},
year = {2013}
}
@article{Cakara2003,
abstract = {Poly(amidoamine) (PAMAM) dendrimers of generations G0, G1, G2, G3, G4, and G6 are investigated by potentiometric (acid−base) titrations. The data are interpreted with a site binding model, which offers the possibility to model the titration curves for all generations of the dendrimers and to describe all dendrimers within a common parameter set. These parameters involve the microscopic ionization constants for each group in the fully deprotonated state and nearest-neighbor pair interaction parameters. From this model we can further obtain all microscopic ionization constants as well as conditional microstate probabilities. The protonation of PAMAM dendrimers first involves protonation of primary amine groups at the outer rim of the dendrimer at high pH, while the tertiary amine groups in the dendrimer core protonate at lower pH. The last group to protonate at low pH is a central tertiary amine.},
author = {Cakara, Dusko and Kleimann, J{\"{o}}rg and Borkovec, Michal},
doi = {10.1021/ma0300241},
file = {:home/mayk/Mendeley Desktop/Cakara, Kleimann, Borkovec/2003/Cakara, Kleimann, Borkovec - 2003 - Microscopic protonation equilibria of poly(amidoamine) dendrimers from macroscopic titrations.pdf:pdf},
issn = {00249297},
journal = {Macromolecules},
number = {11},
pages = {4201--4207},
title = {{Microscopic protonation equilibria of poly(amidoamine) dendrimers from macroscopic titrations}},
volume = {36},
year = {2003}
