@article{Zhou2006,
abstract = {Brownian dynamics simulation is conducted to investigate the dynamics$\backslash$nof dendrimer-polymer conjugates on the basis of a coarse-grained$\backslash$nbead-spring model. The grafted polymers are found to slow the global$\backslash$ndynamics of the dendrimer core. Because of the structural difference$\backslash$nbetween the two moieties, the translational and rotational diffusivities$\backslash$nof a conjugate show different scaling behaviors from those for a$\backslash$ndendrimer, but in principle follow a Stokes-Einstein fashion, analogous$\backslash$nto polymers of other types. When the grafted chains are long enough,$\backslash$nthey become brushlike, evidenced by the scaling behavior of the end-to-end$\backslash$ndistance as well as the chain-length dependence of the rotational$\backslash$nrelaxation time. For the range of chain length investigated, it is$\backslash$nfound that the brushlike grafted polymers still fall within the dilute$\backslash$nregime. Thereby the interchain interactions are not strong enough$\backslash$nto render a change in the scaling behavior of rotation relaxation$\backslash$nin comparison with an isolated rodlike polymer.},
author = {Zhou, Tong and Chen, Shing Bor},
doi = {10.1021/ma061103q},
file = {:home/mayk/Mendeley Desktop/Zhou, Chen/2006/Zhou, Chen - 2006 - A simulation study on dynamics of dendrimer-polymer conjugates.pdf:pdf},
issn = {00249297},
journal = {Macromolecules},
number = {19},
pages = {6686--6692},
title = {{A simulation study on dynamics of dendrimer-polymer conjugates}},
volume = {39},
year = {2006}
}
