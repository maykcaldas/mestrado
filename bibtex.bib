issn = {0024-9297},
journal = {Macromolecules},
number = {2},
pages = {456--461},
title = {{The Molecular Characteristics of Poly(propyleneimine) Dendrimers As Studied with Small-Angle Neutron Scattering, Viscosimetry, and Molecular Dynamics}},
url = {http://pubs.acs.org/doi/abs/10.1021/ma9618181},
volume = {31},
